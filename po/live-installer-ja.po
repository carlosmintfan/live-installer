msgid "Mount point"
msgstr "マウントポイント"

#: usr/lib/live-installer/frontend/gtk_interface.py:338
msgid "Format as"
msgstr "フォーマット形式"

#: usr/lib/live-installer/frontend/gtk_interface.py:339
msgid "Size"
msgstr "サイズ"

#: usr/lib/live-installer/frontend/gtk_interface.py:340
